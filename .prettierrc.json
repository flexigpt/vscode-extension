{
    "endOfLine": "lf",
    "semi": true,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "tabWidth": 2,
    "trailingComma": "none",
    "importOrder": [
      "^(react/(.*)$)|^(react$)|^(react(.*)$)",
      "^(grommet/(.*)$)|^(grommet$)|^(grommet(.*)$)",
      "^(@/logger/(.*)$)|^(@/logger$)",
      "^(@/spec/(.*)$)|^(@/spec$)",
      "^(@/aiprovider/(.*)$)|^(@/aiprovider$)",
      "^(@/prompts/(.*)$)|^(@/prompts$)",
      "^(@/conversations/(.*)$)|^(@/conversations$)",
      "^(@/reactui/lib/(.*)$)|^(@/reactui/lib$)",
      "^(@/reactui/components/(.*)$)|^(@/reactui/components$)",
      "<THIRD_PARTY_MODULES>",
      "",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrderParserPlugins": ["typescript", "tsx", "jsx", "decorators-legacy"]
  }